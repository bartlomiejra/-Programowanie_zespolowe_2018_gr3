<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="244.0" prefWidth="271.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="wu.Oceny_DziekanatController">
   <children>
      <ImageView fitHeight="404.0" fitWidth="682.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@tlo2.jpg" />
         </image>
      </ImageView>
      <TableView fx:id="table_oceny" layoutX="209.0" layoutY="51.0" prefHeight="298.0" prefWidth="437.0">
         <columns>
            <TableColumn fx:id="columnProwadzacy" prefWidth="114.0" resizable="false" text="Prowadzący" />
            <TableColumn fx:id="columnPrzedmiot" prefWidth="133.0" resizable="false" text="Przedmiot" />
            <TableColumn fx:id="columnOcena" prefWidth="50.0" resizable="false" text="Ocena" />
            <TableColumn fx:id="columnStudent" prefWidth="139.0" resizable="false" text="Student" />
         </columns>
      </TableView>
      <Button fx:id="assesment_menu" layoutX="577.0" layoutY="14.0" mnemonicParsing="false" onAction="#assesmentDziekanat_menuActionButton" style="-fx-background-color: #4ca3ba;" text="Menu" textFill="WHITE" />
      <VBox alignment="TOP_CENTER" layoutX="14.0" layoutY="64.0" prefHeight="298.0" prefWidth="169.0" spacing="5.0">
         <children>
            <Label text="Dodaj oceny:" textFill="WHITE" />
            <Label text="Prowadzący:" textFill="WHITE" />
            <ComboBox fx:id="comboProwadzacy" onAction="#comboPDziekanatBtn" prefWidth="150.0" />
            <Label text="Przedmiot:" textFill="WHITE" />
            <ComboBox fx:id="comboPrzedmiot" onAction="#comboPrzDziekanatBtn" prefWidth="150.0" />
            <Label text="Ocena:" textFill="WHITE" />
            <ComboBox fx:id="comboOcena" onAction="#comboODziekanatBtn" prefWidth="150.0" />
            <Label text="Student:" textFill="WHITE" />
            <ComboBox fx:id="comboStudent" onAction="#comboSDziekanatBtn" prefWidth="150.0" />
            <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0" spacing="4.0">
               <children>
                  <Button fx:id="add_ocenaDziekanat" mnemonicParsing="false" onAction="#add_ocenaDziekanatBtn" style="-fx-background-color: #4ca3ba;" text="Dodaj" textFill="WHITE" />
                  <Button fx:id="edit_ocenaDziekanat" mnemonicParsing="false" onAction="#edit_ocenaDziekanatBtn" style="-fx-background-color: #4ca3ba;" text="Edytuj" textFill="WHITE" />
                  <Button fx:id="delete_ocenaDziekanat" mnemonicParsing="false" onAction="#delete_ocenaDziekanatBtn" style="-fx-background-color: #4ca3ba;" text="Usuń" textFill="WHITE" />
               </children>
               <padding>
                  <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
               </padding>
            </HBox>
         </children>
         <padding>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </padding>
      </VBox>
      <Button fx:id="generate_dziekanat" layoutX="364.0" layoutY="362.0" mnemonicParsing="false" onAction="#generate_dziekanatButtonAction" style="-fx-background-color: #4ca3ba;" text="Generuj do PDF" textFill="WHITE" />
   </children>
</AnchorPane>
